.pages-plus-index {
    height: 100%;
    width: 100%;
}

.pages-plus-index .header {
    height: 20rpx;
    background-color: #f8f8f8;
}

.pages-plus-index .plus-content {
    background-color: #fff;
}


.pages-plus-index .plus-content .limits-box .plus-title {
    display: flex;
    justify-content: center;
    align-items: center;
    padding: 36rpx 0;
}

.pages-plus-index .plus-content .limits-box .plus-title .text,
.pages-plus-index .plus-content .limits-box .limits .text {
    font-size: 36rpx;
    color: #333333;
    font-weight: bold;
}

.pages-plus-index .plus-content .limits-box .plus-title .img {
    width: 40rpx;
    height: 40rpx;
    object-fit: cover;
    padding-left: 10rpx;
}

.pages-plus-index .plus-content .limits-box .limits {
    display: flex;
    justify-content: center;
    align-items: center;
    margin-left: -50rpx;
    padding-bottom: 20rpx;
}

.pages-plus-index .plus-content .limits-box .limits .img {
    width: 35rpx;
    height: 30rpx;
    object-fit: cover;
}

.pages-plus-index .plus-content .limits-box .limits .text {
    font-size: 34rpx;
    padding-left: 8rpx;
}

.each-limits {
    padding: 20rpx 30rpx 0rpx 56rpx;
}

.each-limits .up {
    display: flex;
}

.each-limits .up .img {
    width: 50rpx;
    height: 30rpx;
    object-fit: cover;
    margin-top: 7rpx;
}

.each-limits .up .text {
    font-size: 28rpx;
    color: #333333;
    letter-spacing: 2rpx;
    /* font-weight: bold; */
    line-height: 40rpx;
    width: 780rpx;
    padding-left: 10rpx;
}

.each-limits .down {
    font-size: 26rpx;
    color: #999;
    letter-spacing: 2rpx;
    text-align: right;
    padding-top: 10rpx;
}

.pages-plus-index .plus-content .limits-box .vip-fee {
    margin-top: 40rpx;
    padding: 30rpx;
    border-top: 1rpx solid #eee;
}

.pages-plus-index .plus-content .limits-box .vip-fee .fee-box {
    width: 694rpx;
    /* height: 600rpx; */
    background: #f2f5f8;
    border-radius: 22rpx;
}

.vip-fee .fee-box .fee-tit {
    font-size: 32rpx;
    color: #333;
    letter-spacing: 2rpx;
    font-weight: bold;
    padding-left: 28rpx;
    padding-top: 25rpx;
}

.van-radio__label {
    color: #333;
    font-size: 28rpx;
}

.vip-fee .fee-box .fee-price {
    padding-left: 30rpx;
    margin-top: 20rpx;
    color: #333;
    padding-bottom: 30rpx;
}

.vip-fee .fee-box .fee-price .lh {
    letter-spacing: 2rpx;
    padding-left: 60rpx;
    font-size: 26rpx;
    color: #999;
    margin-top: 6rpx;
}

.radio {
    margin-bottom: 20rpx;
    font-size: 30rpx;
}

.van-radio {
    margin-top: 20rpx;
}

.vip-fee .fee-box .fee-price1 {
    padding-left: 30rpx;
    display: flex;
    padding-top: 20rpx;
    padding-bottom: 30rpx;
}

.vip-fee .fee-box .fee-price1 .vip-type {
    display: flex;
    align-items: center;
    padding-right: 28rpx;
}

.vip-fee .fee-box .fee-price1 .vip-type .img {
    width: 40rpx;
    height: 40rpx;
    object-fit: cover;
}

.vip-fee .fee-box .fee-price1 .vip-type .type {
    font-size: 28rpx;
    color: #333333;
    letter-spacing: 2rpx;
    padding-left: 8rpx;
}

.button-btn-box {
    margin-top: 40rpx;
}

.button-btn-box .button-btn-text {
    width: 750rpx;
    height: 92rpx;
    font-size: 28rpx;
    line-height: 92rpx;
    text-align: center;
    color: #fff;
    background-color: #4a90e2;
    transition: all 0.2s;
}

.back-box {
    height: 200rpx;
    display: flex;
    justify-content: center;
    align-items: center;
}

.footer-box {
    margin-top: 120rpx;
    text-align: center;
}

.footer-box .logo-img {
    width: 100rpx;
    height: 100rpx;
    border-radius: 50rpx;
}

.footer-box .name {
    padding-top: 14rpx;
    font-size: 30rpx;
}

.footer-box .tip {
    padding-top: 10rpx;
    color: #9a9a9a;
    font-size: 26rpx;
}
